{
  "name": "doodle-providers",
  "version": "1.0.0",
  "description": "providers for Doodle-Movie-App",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "node test-providers.js",
    "build": "node build-simple.js",
    "dev": "node dev-server.js",
    "dev:build": "npm run build && npm run dev",
    "watch": "nodemon",
    "watch:dev": "nodemon --watch providers --ext ts --exec \"npm run build\" & npm run dev",
    "dev:auto": "concurrently \"npm run watch\" \"npm run dev\"",
    "auto": "node auto-dev.js",
    "start": "npm run build && npm run dev"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/node": "^24.0.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "nodemon": "^3.1.10",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "react-native-aes-crypto": "^3.2.1",
    "rimraf": "^6.0.1"
  }
}
